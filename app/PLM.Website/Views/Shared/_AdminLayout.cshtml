                            <a href="#" class="nav-link">
                                <i class="nav-icon far fa-plus-square"></i>
                                <p>
                                    仓库
                                    <i class="fa fa-angle-left right"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="#" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>
                                            商品管理
                                            <i class="fa fa-angle-left right"></i>
                                        </p>
                                    </a>
                                    <ul class="nav nav-treeview">
                                        <li class="nav-item">
                                            <a asp-controller="@ControllerConstants.Merchandise.Controller" asp-action="@ControllerConstants.Merchandise.Actions.Index" class="nav-link">
                                                <i class="far  nav-icon"></i>
                                                <p>商品管理</p>
                                            </a>
                                        </li>
                                        <li class="nav-item">
                                            <a asp-controller="" asp-action="" class="nav-link">
                                                <i class="far  nav-icon"></i>
                                                <p>包才管理</p>
                                            </a>
                                        </li>
                                    </ul>
                                </li>

                                <li class="nav-item">
                                    <a href="#" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>
                                            自营仓库
                                            <i class="fa fa-angle-left right"></i>
                                        </p>
                                    </a>
                                    <ul class="nav nav-treeview">
                                        <li class="nav-item">
                                            <a asp-controller="r" asp-action="" class="nav-link">
                                                <i class="far fa--dot-circle nav-icon"></i>
                                                <p>仓库清单</p>
                                            </a>
                                        </li>
                                        <li class="nav-item">
                                            <a asp-controller="" asp-action="" class="nav-link">
                                                <i class="far fa--dot-circle nav-icon"></i>
                                                <p>出入记录</p>
                                            </a>
                                        </li>
                                        <li class="nav-item">
                                            <a asp-controller="" asp-action="" class="nav-link">
                                                <i class="far fa--dot-circle nav-icon"></i>
                                                <p>仓库调拨</p>
                                            </a>
                                        </li>
                                        <li class="nav-item">
                                            <a asp-controller="" asp-action="" class="nav-link">
                                                <i class="far fa--dot-circle nav-icon"></i>
                                                <p>仓库盘点</p>
                                            </a>
                                        </li>
                                        <li class="nav-item">
                                            <a asp-controller="" asp-action="" class="nav-link">
                                                <i class="far fa--dot-circle nav-icon"></i>
                                                <p>仓库设置</p>
                                            </a>
                                        </li>
                                        <li class="nav-item">
                                            <a asp-controller="" asp-action="" class="nav-link">
                                                <i class="far fa--dot-circle nav-icon"></i>
                                                <p>货架管理</p>
                                            </a>
                                        </li>
                                    </ul>
                                </li>

                                <li class="nav-item">
                                    <a asp-controller="" asp-action="" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>收货管理</p>
                                    </a>
                                </li>

                                <li class="nav-item">
                                    <a asp-controller="" asp-action="" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>入库管理</p>
                                    </a>
                                </li>
                            </ul>
                        </li>

                    </ul>
                </nav>
            </div>
